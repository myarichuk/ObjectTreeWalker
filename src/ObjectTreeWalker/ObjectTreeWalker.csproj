<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;netstandard2.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <Title>Object Tree Walker</Title>
    <Description>A simple utility class that allows traversing over C# object properties, Node.js style</Description>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/myarichuk/ObjectTreeWalker</RepositoryUrl>
    <PackageTags>object graph;property iteration;property walker;property enumerator;enumerator;object enumerate;object;iterate;iterator</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ObjectTreeWalker.csproj.nuget.dgspec.json" />
    <None Remove="ObjectTreeWalker.csproj.nuget.g.props" />
    <None Remove="ObjectTreeWalker.csproj.nuget.g.targets" />
    <None Remove="project.assets.json" />
    <None Remove="project.nuget.cache" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\CHANGELOG.md" Link="CHANGELOG.md" />
  </ItemGroup>

    <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.ObjectPool" Version="*" />
    <PackageReference Include="Sigil" Version="*" />
    <PackageReference Include="System.Memory" Version="4.5.5" />
  </ItemGroup>

</Project>
